<h1><%=identifier%>'s Data!!!!</h1>
    <table width=300 border=1>
    <tr>
      <td width="200">Site URL</td>
      <td>Count</td>
    </tr>
    <tr>
      <% data.order(Sequel.desc(:count)).each do |url| %>
      <td><%= url[:site_url] %></td>
      <td> <%=  url[:count] %></td> <br/></tr>
      <% end %>
    <br/>
  </table>
  <table width=300 border=1>
    <tr>
      <td width="200">Browsers</td>
      <td>OS</td>
    </tr>
    <tr>
      <% data.each do |url| %>
      <td><%= url %></td>
      <td> <%=  url[:count] %></td> <br/></tr>
      <% end %>
      <br/>
    </table>
